{
  "name": "ts-api-boilerplate",
  "version": "1.0.0",
  "description": "Typescript boilerplate for creating API's",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && tsc",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}'",
    "lint:quiet": "npm run lint -- --quiet",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base --npm r--list-different \"src/**/*.{js,ts}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{js,ts}\"",
    "start": "node -r dotenv/config dist/index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "typescript",
    "express",
    "docker",
    "jest"
  ],
  "author": "Cesar Pe√±a",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/morgan": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "coveralls": "^3.0.9",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
